  "scripts": {
    "start": "react-scripts start",
    "build": " react-scripts build && npm run build:bundle",
    "build-beta": "env-cmd -f ./.env.beta react-app-rewired  build && npm run build:bundle-beta",
    "build-closebeta": "env-cmd -f ./.env.closebeta react-app-rewired  build && npm run build:bundle-closebeta",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "build-css": " postcss src/style/tailwind.css -o src/style/main.css",
    "build-css-watch": " SET TAILWIND_MODE=watch&&postcss src/style/tailwind.css -o src/style/main.css --watch",
    "build:bundle": " webpack --config webpack.config.js ",
    "build:bundle-closebeta": "env-cmd -f ./.env.closebeta  webpack --config webpack.config.js ",
    "build:bundle-beta": "env-cmd -f ./.env.beta webpack --config webpack.config.js "
  },
  "browserslist": {
